    <!-- Our content here -->
      <main>
        {{#each profiles}}
         <div class="col s12 s7">
            <div class="card horizontal">
              <div class="card-image">
                <img src="{{this.photo}}">
              </div>
              <div class="card-stacked">
                <div class="card-content">
                    <h5 class="your-name">{{this.first_name}} {{this.last_name}}</h5>
                    <div class="row">
                        <div class="col s6">
                            <p>Pet {{this.Pet}}</p>
                            <p>Breed: {{this.PetBreed}}</p>
                            <p>Age: {{this.PetAge}}</p>
                        </div>
                        <div class="col s6">
                            <p>Vet: {{this.PetVet}} lbs</p>
                            <p>Temperament: {{this.PetCare}}</p>
                        </div>
                    </div>
                </div>
                <div class="">
                    <p class="black-text text-darken-4">Pet<a href="profile.html"><i class="material-icons right tooltipped" data-position="top" data-delay="50" data-tooltip="more info">more_horiz</i></a></p>
                </div>
              </div>
            </div>
          </div>
          <script>
              var age = "";
              function calcAge(dateString) {
                var birthday = +new Date(dateString);
                return ~~((Date.now() - birthday) / (31557600000));

                }
                calcAge(this.birthdate);

          </script>
        {{/each}}
    </main>
